{
  "name": "@tabler/flags",
  "version": "1.0.0",
  "scripts": {
    "build": "pnpm run clean && pnpm run copy && node ./build.mjs",
    "copy": "pnpm run copy:license",
    "copy:license": "cp ../../LICENSE ./LICENSE",
